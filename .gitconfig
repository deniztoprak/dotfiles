[user]
	name = *** ***
	email = ***.***@***.**
[core]
	editor = vim
	pager = diff-so-fancy | less --tabs=4 -RFX
[push]
	default = simple
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[merge]
	tool = vimdiff
	conflictstyle = diff3
[mergetool]
	prompt = false
	keepBackup = false
[oh-my-zsh]
	hide-status = 1
